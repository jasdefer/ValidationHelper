<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.3</Version>
    <Authors>Justus Bonz</Authors>
    <PackageProjectUrl>https://github.com/jasdefer/ValidationHelper</PackageProjectUrl>
    <Description>This package provides objects which can be returned by methods. Those objects contain feedback about the method call. They can contain error messages or a requested object.</Description>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReleaseNotes>Added the feature to join the errors of a ValidationInfo.</PackageReleaseNotes>
  </PropertyGroup>

</Project>
