<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.5</Version>
    <Authors>Justus Bonz</Authors>
    <PackageProjectUrl>https://github.com/jasdefer/ValidationHelper</PackageProjectUrl>
    <Description>This package provides objects which can be returned by methods. Those objects contain feedback about the method call. They can contain error messages or a requested object.</Description>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReleaseNotes>A validation info can now assimilate another validation info. The assimilated validation info will not change. All error, warning, info and success messages will be copied to the validation info.</PackageReleaseNotes>
  </PropertyGroup>

</Project>
